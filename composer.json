{
    "name": "pluginslab/woocommerce-blocks-example",
    "description": "This is a demo plugin to exemplify how WooCommerce Blocks cart and checkout blocks can be implemented and adopted.",
    "homepage": "https://github.com/pluginslab/pl-gutenberg-boilerplate",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "archive": {
        "exclude": [
            "!/build",
            "!/assets",
            "!/languages"
        ]
    },
    "require-dev": {
        "woocommerce/woocommerce-sniffs": "^0.1.2",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
